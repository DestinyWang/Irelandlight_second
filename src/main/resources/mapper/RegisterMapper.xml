<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.irelandlight.dao.RegisterMapper">

    <insert id="addUser" parameterType="com.irelandlight.model.vo.User">
     INSERT INTO tb_consumer(user_name,password,telephone,birthday,default_address_id)
      VALUES(#{userName},#{password},#{telephone},#{birthday},0);
    </insert>

</mapper>